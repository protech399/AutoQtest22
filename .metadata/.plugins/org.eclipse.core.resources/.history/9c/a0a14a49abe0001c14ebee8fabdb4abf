
public class ifElse1 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=10, b=20, temp;
		temp=a;
		a=b;
		b=temp;
		System.out.println(a);
		System.out.println(b);
		a=a+b;
		b=a-b;
		b=a-b;
		System.out.println(a);
		System.out.println(b);
	}

}
